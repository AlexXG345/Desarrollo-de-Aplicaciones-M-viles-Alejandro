<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alex.tiendaonline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TiendaOnline"
        android:theme="@style/Theme.TiendaOnline">


        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.TiendaOnline">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".WelcomeActivity" android:exported="false" />
        <activity android:name=".LoginActivity" android:exported="false" />
        <activity android:name=".RegisterActivity" android:exported="false" />
        <activity android:name=".ProfileActivity" android:exported="false" />

    </application>
</manifest>